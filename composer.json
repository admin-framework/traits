{
  "name": "admin-framework/traits",
  "description": "AdminFramework Traits 提供了一组实用的 PHP Trait，用于快速实现常见的设计模式和功能特性。",
  "type": "library",
  "license": "MIT",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Admin Framework",
      "email": "admin@adminframework.com",
      "homepage": "https://www.adminframework.com"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "AdminFramework\\Traits\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AdminFramework\\Traits\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit tests/",
    "cs-check": "phpcs --standard=PSR12 src/ tests/",
    "cs-fix": "phpcbf --standard=PSR12 src/ tests/"
  }
}
